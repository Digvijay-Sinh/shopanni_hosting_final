              <div class="row">
                <div class="card">
                  <h5 class="card-header">Orders</h5>
                  <div class="table-responsive text-nowrap">
                    <table id="ordersTable" class="table">
                      <thead>
                        <tr class="text-nowrap">
                          <th>#</th>
                          <th>Order ID</th>
                          <th>User ID</th>
                          <th>Name</th>
                          <th>Address</th>
                          <th>District</th>
                          <th>State</th>
                          <th>Phone</th>
                          <th>Email</th>
                          <th>Total</th>
                          <th>Created At</th>
                          <th>Updated At</th>
                          <th>Landmark</th>
                          <th>Razorpay Payment ID</th>
                          <th>Razorpay Order ID</th>
                          <th>Razorpay Signature</th>
                          <th>Status</th>
                          <th>Action</th>
                        </tr>
                      </thead>
                      <tbody id="ordersTableBody"></tbody>
                    </table>
                  </div>
                </div>
